{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "swiftbuild",
			"type": "process",
			"command": "swiftc",
			"args": [
				"${file}",
				"-oout.exe"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": false,
				"clear": false
			}
		}
	]
}